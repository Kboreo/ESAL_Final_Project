  SysCtlClockSet(0x07800000 | 0x00003800 |   0x00000000 | 0x00000540);
	
	
  SysCtlPWMClockSet(0x00000000);
	
	
  SysCtlPeripheralEnable(0xf0000805);
  SysCtlPeripheralEnable(0xf0004001);  
	
	
	
	while(!SysCtlPeripheralReady(0xf0004001))
